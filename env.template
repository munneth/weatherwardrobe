# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Google Gemini API Key (get from https://makersuite.google.com/app/apikey)
gemini_api_key=your_gemini_api_key_here

# Weather API Key
key=your_weather_api_key_here

# Supabase Database
NEXT_PUBLIC_SUPABASE_KEY=your_supabase_anon_key_here

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here

# AI Outfit Generation Service
AI_OUTFIT_API_KEY=your_ai_outfit_api_key_here
AI_OUTFIT_API_URL=https://your-ai-api-endpoint.com/generate-outfits 